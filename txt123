repeat wait(.5)
   for i,v in pairs(getgc()) do
       if typeof(v) == 'function' then
           pcall(function()
               for b,c in pairs(debug.getconstants(v)) do
                   if c == 2.7 and b == 1 and debug.getinfo(v)['nups'] == 1 then
                       _G.Wtf = function(mult)
                           debug.setconstant(v, 1, 2.7 * mult)
                       end
                       break
                   end
               end
           end)
       end
   end
until _G.Wtf

_G.Wtf(1.5) -- change the number to increase your gain, for example 1 is normal gain. To look more legit make it 1.1-1.4
